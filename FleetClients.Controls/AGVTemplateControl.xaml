<UserControl x:Class="FleetClients.Controls.AGVTemplateControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FleetClients.Controls"
             xmlns:gaCoreC="clr-namespace:GACore.Controls;assembly=GACore.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/GACore.Controls;component/ResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <StackPanel>
        <gaCoreC:LabelContentControl Header="IPAddress:">
            <TextBox Text="{Binding Path=IPV4String}"/>
        </gaCoreC:LabelContentControl>
        <gaCoreC:LabelContentControl Header="Pose:">
            <TextBox Text="{Binding Path=PoseDataString}"/>
        </gaCoreC:LabelContentControl>
    </StackPanel>
</UserControl>
